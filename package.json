{
  "name": "assethub-enterprise",
  "version": "1.0.0",
  "private": true,
  "description": "Comprehensive Enterprise Inventory & Asset Management System with Mobile Support, Finance Tracking, and GDPR Compliance",
  "keywords": [
    "asset-management",
    "inventory",
    "enterprise",
    "react",
    "nextjs",
    "typescript",
    "prisma",
    "postgresql",
    "mobile",
    "gdpr",
    "finance",
    "depreciation"
  ],
  "author": "AssetHub Team <it@company.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/company/assethub-enterprise.git"
  },
  "bugs": {
    "url": "https://github.com/company/assethub-enterprise/issues"
  },
  "homepage": "https://assethub.company.com",
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=8.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest --coverage --passWithNoTests --watchAll=false",
    "test:unit": "jest --testPathPattern=unit --watchAll=false",
    "test:integration": "jest --testPathPattern=integration --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false --coverageReporters=text-lcov",
    "test:a11y": "jest --config jest.a11y.config.js",
    "security:scan": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:format": "prisma format",
    "docker:dev": "docker-compose up -d",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:build": "docker build -t assethub:latest .",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "analyze": "ANALYZE=true next build",
    "lighthouse": "lighthouse-ci",
    "prepare": "husky install",
    "postinstall": "prisma generate",
    "clean": "rimraf .next out dist coverage",
    "preview": "npm run build && npm run start",
    "deploy:staging": "echo 'Deploy to staging environment'",
    "deploy:production": "echo 'Deploy to production environment'"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.87.1",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.6.0",
    "html2canvas": "^1.4.1",
    "jose": "^5.10.0",
    "jspdf": "^2.5.2",
    "jsqr": "^1.4.0",
    "lucide-react": "^0.509.0",
    "nanoid": "^5.1.5",
    "next": "15.3.2",
    "next-themes": "^0.4.6",
    "nodemailer": "^6.10.1",
    "papaparse": "^5.5.3",
    "qrcode": "^1.5.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-qr-scanner": "^1.0.0-alpha.11",
    "recharts": "^2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "uuid": "^10.0.0",
    "web-push": "^3.6.7",
    "xlsx": "^0.18.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.19.13",
    "@types/nodemailer": "^6.4.19",
    "@types/papaparse": "^5.3.16",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/uuid": "^10.0.0",
    "@types/web-push": "^3.6.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.34.0",
    "eslint-config-next": "15.3.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "prisma": "^5.22.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}